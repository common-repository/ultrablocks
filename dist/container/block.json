{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ultrablocks/container",
  "version": "0.0.1",
  "title": "Container",
  "category": "ultrablocks",
  "description": "Advanced, powerful containers designed for responsive CSS layouts.",
  "example": {},
  "attributes": {
    "blockId": {
      "type": "string"
    },
    "tagName": {
      "type": "string",
      "default": "div"
    },
    "style": {
      "type": "object",
      "default": {
        "backgroundColor": "#f6f6f6",
        "padding": "20px",
        "margin": "0"
      }
    }
  },
  "textdomain": "ultrablocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.css",
  "style": [
    "style-ultrablocks",
    "file:./style.css"
  ],
  "supports": {
    "className": true
  }
}