{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ultrablocks/search",
  "version": "0.0.2",
  "title": "Search",
  "category": "ultrablocks",
  "description": "Search block for better search experience.",
  "example": {},
  "attributes": {
    "blockId": {
      "type": "string"
    },
    "layoutType": {
      "type": "string",
      "default": "button-inside"
    },
    "placeholder": {
      "type": "string",
      "default": "Enter your query here"
    },
    "buttonType": {
      "type": "string",
      "default": "icon"
    },
    "showIcon": {
      "type": "boolean",
      "default": true
    },
    "buttonIcon": {
      "type": "string",
      "default": "<svg class=\"ultrablocks-svg-icon ultrablocks-svg-icon-fa\" aria-hidden=\"true\" role=\"img\" xmlns=\"http://www.w3.org/2000/svg\" viewBox=\"0 0 512 512\"><path fill=\"currentColor\" d=\"M416 208c0 45.9-14.9 88.3-40 122.7L502.6 457.4c12.5 12.5 12.5 32.8 0 45.3s-32.8 12.5-45.3 0L330.7 376c-34.4 25.2-76.8 40-122.7 40C93.1 416 0 322.9 0 208S93.1 0 208 0S416 93.1 416 208zM208 352a144 144 0 1 0 0-288 144 144 0 1 0 0 288z\"></path></svg>"
    },
    "buttonIconPosition": {
      "type": "string",
      "default": "left"
    },
    "showButtonText": {
      "type": "boolean",
      "default": true
    },
    "buttonText": {
      "type": "string",
      "default": "Search"
    },
    "style": {
      "type": "object",
      "default": {}
    },
    "inputStyle": {
      "type": "object",
      "default": {},
      "selector": ".ub-search-control .ub-form-input"
    },
    "inputStyleFocus": {
      "type": "object",
      "default": {},
      "selector": ".ub-search-control .ub-form-input:focus"
    },
    "buttonStyle": {
      "type": "object",
      "default": {},
      "selector": ".ub-form-submit-btn .ub-form-btn"
    },
    "buttonStyleHover": {
      "type": "object",
      "default": {},
      "selector": ".ub-form-submit-btn:hover .ub-form-btn"
    },
    "styleButtonGap": {
      "type": "object",
      "default": {},
      "selector": ".ub-form-submit-btn .ub-form-btn"
    },
    "buttonIconSpacing": {
      "type": "object",
      "default": {},
      "selector": ".ub-search-control .ultrablocks-component-icon-picker-preview"
    }
  },
  "textdomain": "ultrablocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css",
  "supports": {
    "className": true,
    "html": false,
    "anchor": true,
    "align": true,
    "alignWide": true
  }
}